{
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "always-multiline"
        }],
        "eol-last": ["error", "always"],
        "key-spacing": ["error", { "beforeColon": false }],
        "space-before-blocks": ["error", "always"],
        "object-curly-spacing": ["error", "always"],
        "array-bracket-spacing": ["error", "never"]
    },
    "parser": "babel-eslint"
}